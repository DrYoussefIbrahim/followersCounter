/*
  Intstagram followers counter. Sets up WiFi AP to configure account and WiFi settings through a captive portal.
  Stores credintials in non-volatile memory for subsequent uses.
  This accompanies the NANO firmware file NANO_FOLLOWERS
  by Youssef Ibrahim on 25 September, 2020
*/

#include <WiFi.h>
#include <WiFiClient.h>
#include <WiFiClientSecure.h>
#include <HardwareSerial.h>
#include <WiFiAP.h>
#include <stdio.h>
#include <string.h>
#include <EEPROM.h>
#include <DNSServer.h>
#include "InstagramStats.h"
#include "JsonStreamingParser.h"

#define EEPROM_SIZE 500

WiFiServer server(80);
const byte DNS_PORT = 53;
IPAddress apIP(172, 217, 28, 1);
DNSServer dnsServer;
HardwareSerial nano(2);
int n = 0;
int id = 0;
String passwd = "";
String insta = "";
String net = "";
char clean_pwd[64];
char NET[64];
char INST[64];
int page = 0;
boolean saved = 1;
boolean volatile pres = 0;
unsigned long color = 1000000;
unsigned long delayBetweenChecks = 30000; //mean time between api requests
unsigned long whenDueToCheck = 0;

void IRAM_ATTR isr() {
  pres = 1;
}

void setup() {
  pinMode(18, INPUT_PULLUP);
  pinMode(19, OUTPUT);
  digitalWrite(19, LOW);
  pinMode(4, OUTPUT);
  digitalWrite(4, LOW);
  pinMode(5, OUTPUT);
  digitalWrite(5, HIGH);
  nano.begin(9600, SERIAL_8N1, 16, 17);
  Serial.begin(115200);
  EEPROM.begin(EEPROM_SIZE);
  attachInterrupt(18, isr, FALLING);
  ledcSetup(0, 5000, 8);
  ledcSetup(1, 5000, 8);
  ledcAttachPin(4, 0); //RED LED
  ledcAttachPin(19, 1); //GREEN LED
}

void loop() {
  digitalWrite(5, LOW);
  delay(1000);
  nano.println(0);
  delay(1000);
  digitalWrite(5, HIGH);
  pres = 0;
  if (!saved) {
    for (byte level = 0; level < 255; level++) {
      ledcWrite(0, level);
      ledcWrite(1, level / 4);
      delay(1);
    }

    WiFi.disconnect();   //added to start with the wifi off, avoid crashing
    WiFi.mode(WIFI_OFF); //added to start with the wifi off, avoid crashing
    WiFi.mode(WIFI_AP);
    WiFi.softAPConfig(apIP, apIP, IPAddress(255, 255, 255, 0));
    WiFi.softAP("Dr Y's Followers Counter", "password");
    dnsServer.start(DNS_PORT, "*", apIP);
    IPAddress myIP = WiFi.softAPIP();
    Serial.print("AP IP address: ");
    Serial.println(myIP);
    server.begin();
    Serial.println("Server started");
    byte level = 0;

    while (page != 4) {

      dnsServer.processNextRequest();
      WiFiClient client = server.available();   // listen for incoming clients

      if (client) {
        ledcWrite(0, 0);
        ledcWrite(1, 0);
        delay(250);
        ledcWrite(0, 255);
        ledcWrite(1, 255 / 4);
        // if you get a client,
        Serial.println("New Client.");           // print a message out the serial port
        String currentLine = "";                // make a String to hold incoming data from the client
        while (client.connected()) {            // loop while the client's connected
          if (client.available()) {             // if there's bytes to read from the client,
            char c = client.read();             // read a byte, then
            Serial.write(c);                    // print it out the serial monitor
            if (c == '\n') {                    // if the byte is a newline character

              // if the current line is blank, you got two newline characters in a row.
              // that's the end of the client HTTP request, so send a response:
              if (currentLine.length() == 0) {
                if (page == 0) {
                  client.println("HTTP/1.1 200 OK");
                  client.println("Content-type:text/html");
                  client.println();

                  client.print("<head><meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0, user-scalable=no\"></head>");
                  client.print("<h1>Dr Y's Followers Counter</h1>");
                  //client.print("<img src=\"data:image/png;base64,\"");
                  client.print("<form><br>");
                  client.print("<b>Enter Instagram account ID");
                  client.print(":</b> <input type=\"text\" name=\"insta\"><br>");
                  client.print("<input type=\"submit\" value=\"Submit\"><br>");
                  client.print("</form>");
                }
                if (page == 1) {
                  n = WiFi.scanNetworks();

                  client.println("HTTP/1.1 200 OK");
                  client.println("Content-type:text/html");
                  client.println();

                  client.print("<head><meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0, user-scalable=no\"></head>");
                  client.print("<b>Instagram ID:</b> ");
                  client.print(insta);
                  client.print("<br>");
                  client.print("<b>Please select your preferred WiFi network:</b><br>");
                  for (int i = 0; i < n; ++i) {
                    // Print SSID and RSSI for each network found
                    client.print(i + 1);
                    client.print(": ");
                    client.print("<a href=\"/");
                    client.print(i + 1);
                    client.print("\">");
                    client.print(WiFi.SSID(i));
                    client.print("</a>");
                    client.print("<br>");
                  }
                }

                else if (page == 2) {
                  client.println("HTTP/1.1 200 OK");
                  client.println("Content-type:text/html");
                  client.println();

                  client.print("<head><meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0, user-scalable=no\"></head>");
                  client.print("<form><br>");
                  client.print("<b>Enter Password for ");
                  client.print(WiFi.SSID(id));
                  client.print(":</b> <input type=\"text\" name=\"pass\"><br>");
                  client.print("<input type=\"submit\" value=\"Submit\"><br>");
                  client.print("</form>");
                }

                else if (page == 3) {
                  client.println("HTTP/1.1 200 OK");
                  client.println("Content-type:text/html");
                  client.println();

                  client.print("<head><meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0, user-scalable=no\"></head>");
                  client.print("<h1>Connecting....</h1>");
                  page = 4;
                }

                // The HTTP response ends with another blank line:
                client.println();
                // break out of the while loop:
                break;
              } else {    // if you got a newline, then clear currentLine:
                currentLine = "";
              }
            } else if (c != '\r') {  // if you got anything else but a carriage return character,
              currentLine += c;      // add it to the end of the currentLine
            }


            for (int i = 0; i < n; ++i) {
              String temp = "GET /";
              temp.concat(i + 1);
              if (currentLine.endsWith(temp)) {
                Serial.println();
                Serial.println(WiFi.SSID(i));
                page = 2;
                id = i;
              }
            }

            int insta_pres = currentLine.indexOf("insta=");
            if (insta_pres != -1 && page == 0) {
              char tempp = client.read();
              insta = "";
              while (tempp != ' ') {
                insta += tempp;
                tempp = client.read();
              }
              Serial.println();
              Serial.println(insta);
              page = 1;
            }

            int pass_pres = currentLine.indexOf("pass=");
            if (pass_pres != -1 && page == 2) {
              char temp = client.read();
              passwd = "";
              while (temp != ' ') {
                passwd += temp;
                temp = client.read();
              }
              Serial.println();
              Serial.println(passwd);
              page = 3;
            }

          }
        }
        // close the connection:
        client.stop();
        Serial.println("Client Disconnected.");
      }
    }
    page = 0;
    net = String(WiFi.SSID(id));
    Serial.println(net);
    for (byte mem = 0; mem < sizeof(clean_pwd); mem++) {
      clean_pwd[mem] = 0;
    }
    for (byte i = 0; i < passwd.length(); i++) {
      Serial.println(passwd[i]);
      if (passwd[i] == '%') {
        char val = passwd[i + 1] > 0x39 ? (passwd[i + 1] - 'A') * 16 : (passwd[i + 1] - '0') * 16;
        val += passwd[i + 2] > 0x39 ? (passwd[i + 2] - 'A') : (passwd[i + 2] - '0');
        clean_pwd[i] = val;
        i += 3;
      } else {
        clean_pwd[i] = passwd[i];
      }
      Serial.println(clean_pwd[i]);
    }
    Serial.println(clean_pwd);
    Serial.println(passwd);

    EEPROM.write(0, insta.length());
    EEPROM.commit();
    EEPROM.write(1, net.length());
    EEPROM.commit();
    EEPROM.write(2, sizeof(clean_pwd));
    EEPROM.commit();

    int memCount = 3;

    for (byte i = 0; i < insta.length(); i++) {
      EEPROM.write(memCount, insta[i]);
      EEPROM.commit();
      memCount++;
    }
    for (byte i = 0; i < net.length() + 2; i++) {
      EEPROM.write(memCount, net[i]);
      EEPROM.commit();
      memCount++;
    }
    for (byte i = 0; i < sizeof(clean_pwd); i++) {
      EEPROM.write(memCount, clean_pwd[i]);
      EEPROM.commit();
      memCount++;
    }
    net.toCharArray(NET, net.length() + 2);
  } else {
    int memCount = 3;
    for (byte mem = 0; mem < sizeof(INST); mem++) {
      INST[mem] = 0;
    }
    for (byte i = 0; i < EEPROM.read(0); i++) {
      INST[i] = EEPROM.read(memCount);
      Serial.println(EEPROM.read(memCount));
      memCount++;
    }
    for (byte mem = 0; mem < sizeof(NET); mem++) {
      NET[mem] = 0;
    }
    for (byte i = 0; i < EEPROM.read(1) + 2; i++) {
      NET[i] = EEPROM.read(memCount);
      Serial.println(EEPROM.read(memCount));
      memCount++;
    }
    Serial.println(net);
    for (byte mem = 0; mem < sizeof(clean_pwd); mem++) {
      clean_pwd[mem] = 0;
    }
    for (byte i = 0; i < EEPROM.read(2); i++) {
      clean_pwd[i] = EEPROM.read(memCount);
      Serial.println(EEPROM.read(memCount));
      memCount++;
    }
    Serial.println(clean_pwd);
    insta = INST;
  }
  char *ssid = &NET[0];
  char *password = &clean_pwd[0];
  Serial.println(ssid);
  Serial.println(password);
  Serial.println(insta);
  WiFi.softAPdisconnect(true);
  WiFi.begin(ssid, password);
  delay(1000);
  pres = 0;
  saved = 1;
  while (WiFi.status() != WL_CONNECTED && !pres) {
    for (byte level = 0; level < 255; level++) {
      ledcWrite(0, level);
      delay(1);
    }
    for (byte level = 254; level > 0; level--) {
      ledcWrite(0, level);
      delay(1);
    }
    Serial.print(".");
  }

  if (WiFi.status() == WL_CONNECTED) {
    ledcWrite(0, 0);
    for (byte level = 0; level < 50; level++) {
      ledcWrite(1, level);
      delay(2);
    }
    // Print local IP address and start web server
    Serial.println("");
    Serial.println("WiFi connected.");
    Serial.println("IP address: ");
    Serial.println(WiFi.localIP());
    pres = 0;
  } else {
    saved = 0;
    pres = 0;
  }

  WiFiClientSecure client;
  InstagramStats instaStats(client);
  delay(1000);

  while (saved) {
    unsigned long timeNow = millis();
    if ((timeNow > whenDueToCheck) && WiFi.status() == WL_CONNECTED)  {
      Serial.println("Getting instagram user stats for " + insta );
      InstagramUserStats response = instaStats.getUserStats(insta);
      Serial.println("Response:");
      Serial.print("Number of followers: ");
      Serial.println(response.followedByCount);
      if (response.followedByCount) {
        digitalWrite(5, LOW);
        delay(1000);
        nano.println(response.followedByCount);
        delay(1000);
        digitalWrite(5, HIGH);
      } else {
        for ( byte flash = 0; flash < 3; flash++) {
          for (byte level = 0; level < 255; level++) {
            ledcWrite(0, level);
            delay(1);
          }
          for (byte level = 254; level > 0; level--) {
            ledcWrite(0, level);
            delay(1);
          }
        }
      }
      whenDueToCheck = timeNow + delayBetweenChecks;
    }
    if (pres) {
      unsigned long press_time = millis();
      while (! digitalRead(18)) {
        if (millis() - press_time > 3000) {
          saved = 0;
          break;
        }
      }
      if (saved) {
        digitalWrite(5, LOW);
        delay(10);
        nano.println(color);
        if (color < 8000000) {
          color += 1000000;
        } else {
          color = 1000000;
        }
        delay(1000);
        digitalWrite(5, HIGH);
        pres = 0;
      } else {
        digitalWrite(5, LOW);
        for (byte level = 0; level < 255; level++) {
          ledcWrite(0, level);
          ledcWrite(1, level / 4);
          delay(1);
        }
        nano.println(0);
        delay(50);
        digitalWrite(5, HIGH);
      }
    }
    if (WiFi.status() != WL_CONNECTED) {
      ledcWrite(1, 0);
      while (WiFi.status() != WL_CONNECTED && !pres) {
        for (byte level = 0; level < 255; level++) {
          ledcWrite(0, level);
          delay(1);
        }
        for (byte level = 254; level > 0; level--) {
          ledcWrite(0, level);
          delay(1);
        }
      }
      if (pres) {
        saved = 0;
        pres = 0;
      } else {
        ledcWrite(0, 0);
        for (byte level = 0; level < 50; level++) {
          ledcWrite(1, level);
          delay(2);
        }
      }
    }
  }
}
